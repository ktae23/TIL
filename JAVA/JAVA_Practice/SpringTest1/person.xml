<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="person1" class="my.model.PersonImpl">
		<property name="name">
		<value>박경태</value>
		</property>
	</bean>

	<bean id="person2" class="my.model.PersonImpl">
		<constructor-arg value="이순신"></constructor-arg>
	</bean>
	
	<bean id="person3" class="my.model.PersonImpl">
		<constructor-arg value="박경태"></constructor-arg>
		<constructor-arg value="30"></constructor-arg>
	</bean>

	<bean id="person4" class="my.model.PersonImpl">
		<constructor-arg value="50"></constructor-arg>
		<constructor-arg value="이순수"></constructor-arg>
	</bean>
		
	
</beans>
